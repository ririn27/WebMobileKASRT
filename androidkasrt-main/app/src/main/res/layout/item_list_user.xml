<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <ImageView
        android:id="@+id/itemAvatar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_margin="16dp"
        android:layout_marginStart="32dp"
        android:contentDescription="Avatar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.124"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/itemName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:textColor="#000"
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/itemAvatar"
        app:layout_constraintTop_toTopOf="@+id/itemAvatar"
        android:text="Name" />

    <TextView
        android:id="@+id/itemEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/itemName"
        app:layout_constraintTop_toBottomOf="@+id/itemName"
        android:text="Email" />

    <TextView
        android:id="@+id/itemJudulAlamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/itemEmail"
        app:layout_constraintTop_toBottomOf="@+id/itemEmail"
        android:text="Alamat :" />
    <TextView
        android:id="@+id/itemAlamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulAlamat"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulAlamat"
        android:text="Alamat" />

    <TextView
        android:id="@+id/itemJudulIuranIndividu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/itemAlamat"
        app:layout_constraintTop_toBottomOf="@+id/itemAlamat"
        android:text="Iuran Individu :" />
    <TextView
        android:id="@+id/itemIuranIndividu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulIuranIndividu"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulIuranIndividu"
        android:text="Iuran Individu" />

    <TextView
        android:id="@+id/itemJudulIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemIuranIndividu"
        app:layout_constraintTop_toBottomOf="@+id/itemIuranIndividu"
        android:text="Jumlah Iuran  Bulanan :" />
    <TextView
        android:id="@+id/itemIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulIuranBulanan"
        android:text="Iuran Bulanan" />

    <TextView
        android:id="@+id/itemJudulRekapIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemIuranBulanan"
        android:text="Rekap Iuran  Bulanan :" />
    <TextView
        android:id="@+id/itemRekapIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulRekapIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulRekapIuranBulanan"
        android:text="Rekap Iuran Bulanan" />

    <TextView
        android:id="@+id/itemJudulPengeluaranIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemRekapIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemRekapIuranBulanan"
        android:text="Rekap Pengeluaran Iuran  Bulanan :" />
    <TextView
        android:id="@+id/itemPengeluaranIuran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulPengeluaranIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulPengeluaranIuranBulanan"
        android:text="Pengeluaran Iuran Bulanan" />

    <TextView
        android:id="@+id/itemJudulPemanfaatanIuranBulanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#7E7E7E"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemPengeluaranIuran"
        app:layout_constraintTop_toBottomOf="@+id/itemPengeluaranIuran"
        android:text="Pemanfaatan Iuran  Bulanan :" />
    <TextView
        android:id="@+id/itemPemanfaatanIuran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:textColor="#000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/itemJudulPemanfaatanIuranBulanan"
        app:layout_constraintTop_toBottomOf="@+id/itemJudulPemanfaatanIuranBulanan"
        android:text="Pemanfaat Iuran Bulanan" />


</androidx.constraintlayout.widget.ConstraintLayout>